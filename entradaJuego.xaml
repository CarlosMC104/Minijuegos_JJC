      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MiniVerse"
      mc:Ignorable="d" 
      d:DesignHeight="100" d:DesignWidth="300"
      Title="entradaJuego">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/images/banner.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Button x:Name="btnYes" Content="Button" HorizontalAlignment="Left" Margin="34,71,0,0" VerticalAlignment="Top" Width="99"/>
        <Button x:Name="btnNo" Content="Button" HorizontalAlignment="Left" Margin="177,71,0,0" VerticalAlignment="Top" Height="20" Width="99"/>

    </Grid>
</Page>
